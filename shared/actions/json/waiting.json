{
  "prelude": ["import type {RPCError} from '../util/errors'"],
  "actions": {
    "incrementWaiting": {
      "key": ["string", "Array<string>"]
    },
    "decrementWaiting": {
      "key": ["string", "Array<string>"],
      "error?": "RPCError"
    },
    "changeWaiting": {
      "key": ["string", "Array<string>"],
      "increment": "boolean",
      "error?": "RPCError"
    },
    "clearWaiting": {
      "key": ["string", "Array<string>"]
    }
  }
}
